{
  "name": "zthxxx-blog",
  "version": "0.2.0",
  "private": true,
  "description": "blog by zthxxx",
  "scripts": {
    "dev": "npx --node-arg '--require ./inject.js' vuepress dev source --open --temp .temp",
    "build": "npx --node-arg '--require ./inject.js' vuepress build source --temp .temp --dest dist",
    "deploy": "gh-pages -d dist -b master",
    "lint": "eslint --ext .js,.vue source",
    "lint:fix": "eslint --fix --ext .js,.vue source",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.vue",
    "commit": "npx git-cz",
    "prettier": "prettier --write './source/.vuepress/**/*'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zthxxx/zthxxx.github.io.git"
  },
  "keywords": [
    "vue",
    "vuepress",
    "blog"
  ],
  "author": "zthxxx",
  "license": "MIT",
  "homepage": "https://blog.zthxxx.me",
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@vuepress/plugin-google-analytics": "latest",
    "commitizen": "^3.0.7",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "vuepress": "latest",
    "vuepress-theme-cherry": "file:source/.vuepress/theme"
  },
  "lint-staged": {
    "**/*.{js,vue}": "npm run lint-staged:js"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "browserslist": [
    "Chrome >= 70",
    "Firefox >= 66",
    "Safari >= 12"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
